{
  "name": "react-litipsum",
  "version": "1.0.1",
  "description": "A simple react component that fetches placeholder text from litipsum.com.",
  "main": "dist/index.js",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
